{
  "name": "use-tw-zipcode",
  "description": "React hook for getting Taiwan zip code.",
  "keywords": [
    "taiwan",
    "react",
    "hook"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imgarylai/use-tw-zipcode"
  },
  "bugs": {
    "url": "https://github.com/imgarylai/use-tw-zipcode/issues"
  },
  "homepage": "https://github.com/imgarylai/use-tw-zipcode#readme",
  "author": "Gary Lai <garylai1990@gmail.com>",
  "version": "2.0.3",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  },
  "devDependencies": {
    "@commitlint/cli": "13.1.0",
    "@commitlint/config-conventional": "13.1.0",
    "@commitlint/prompt": "13.1.0",
    "@semantic-release/changelog": "6.0.0",
    "@semantic-release/git": "10.0.0",
    "@semantic-release/github": "8.0.1",
    "@types/react": "17.0.24",
    "@typescript-eslint/eslint-plugin": "4.31.2",
    "@typescript-eslint/parser": "4.31.2",
    "commitizen": "4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.24.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.26.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "7.0.2",
    "microbundle": "0.13.3",
    "prettier": "2.4.1",
    "react": "17.0.2",
    "semantic-release": "18.0.0",
    "typescript": "4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        1,
        "always",
        100
      ],
      "footer-max-line-length": [
        1,
        "always",
        100
      ]
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/changelog",
        {
          "changelogFile": "docs/CHANGELOG.md"
        }
      ],
      "@semantic-release/npm",
      "@semantic-release/github",
      [
        "@semantic-release/git",
        {
          "assets": [
            "docs/CHANGELOG.md",
            "package.json",
            "package-lock.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
